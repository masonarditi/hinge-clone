{
  "expo": {
    "name": "Hinge Dev",
    "slug": "hinge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sync-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.hinge"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sync-logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.hinge"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/sync-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sync-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-video",
      "expo-font",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
